  <style>
    #email,
    #username,
    #phone,
    #phoneCertified,
    #gender {
      font-family: NotoSansKR-Regular;
      margin: 20px auto;
      border: 1px solid #8080805c;
      padding: 12px 5px 17px;
      width: 100%;
      float: left;
      color: #5172df;
      widows: 100%;
    }

    #phone{
        width : 60%;
    }

    .info {
      font-weight: bold;
      font-size: 12px;
      color: red;
    }

    label {
      padding: 4px 2px;
      width: 60%;
    }

    input[type="submit"]{
      text-align: center;
      border: 1px solid #8080805c;
      width: 100%;
      padding: 12px 5px 17px;
      margin: 20px 0px;
      text-transform: uppercase;
      transition: all 0.3s ease-in-out;
      cursor: pointer;
    }

    input[name="dropuser"] {
      width: 20%;
      float: left;
    }

    input[type="file"] {
      background-color: #f5f5f5;
      width: 100%;
    }

    #certified{
        margin : 20px 0px;
        padding: 12px 5px 12px;
    }

    .content {
      width: 66%;
      float: left;
    }
    .profile {
      width: 50%;
      display: inline-block;
    }

    .image {
      margin: 20px auto;
      width: 25%;
      height: 25%;
      float: right;
      margin-right: 10%;
      margin-top : 10%;
    }

    .button {
      margin: 0 auto;
      width: 100%;
    }

    #user_image {
      height: 140px;
    }

    #btn {
      margin-top : 5%;
      color: white;
      width : 93%;
      background-color: #5172df;
      border: 1px solid #8080805c;
    }

    h5{
        font-weight: bold;
    }

    #username{
      width : 70%;
    }

    #duplicated{
      width : 20%;
    }

  </style>

